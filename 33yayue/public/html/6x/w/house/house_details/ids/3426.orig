            $(this).removeClass('active').siblings('.optgroup').slideUp(100);
        } else {
            $(this).parents('[js="month-pay"]').find('.mp-select-mask').removeClass('active');    // 删除其他active
            $(this).parents('[js="month-pay"]').find('.optgroup').slideUp(100);                   // 回收其他下拉

            $(this).addClass('active').siblings('.optgroup').slideDown(100);
        }
    });

    $('[js="month-pay"]').on('click', '.mp-form-control .optgroup li', function () {
        var _index = $(this).index();
        var _text = $(this).text();
        $(this).parent().siblings('.mp-select-mask').text(_text).removeClass('active');
        $(this).parent().slideUp(100).siblings('select').children('option').eq(_index).attr('checked', 'checked').siblings().removeAttr('checked');
