        var _top = ($(this).height() - $(this).find('.total-price').height()) / 2 - 10;
        $(this).find('.total-price').css({ 'left': _left, 'top': _top });
    });


    $(document).click(function () {
        $('[js="month-pay"]').find('.optgroup').slideUp(100).siblings('.mp-select-mask').removeClass('active');
    });
</script> 

 
